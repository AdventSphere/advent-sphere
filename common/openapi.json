          },
          "objectThumbnail": {
            "type": "string",
            "format": "binary",
            "description": "アイテムのサムネイル画像ファイル",
            "example": "thumbnail.png"
          }
        }
      },
      "CreateRoomResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "room_12345",
            "description": "ルームID"
          },
          "editId": {
            "type": "string",
            "example": "12345",
            "description": "ルームの編集ID"
          }
        },
        "required": ["id", "editId"]
      },
      "CreateRoom": {
        "type": "object",
        "properties": {
          "ownerId": {
            "type": "string",
            "example": "user_12345",
            "description": "オーナーのユーザーID"
          },
          "itemGetTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T00:00:00Z",
            "description": "アイテム取得日時"
          },
          "password": {
            "type": "string",
            "example": "securepassword",
            "description": "ルームのパスワード"
          },
          "isAnonymous": {
            "type": "boolean",
            "example": false,
            "description": "匿名モードかどうか"
          },
          "startAt": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T00:00:00Z",
            "description": "開始日時"
          }
        },
        "required": ["ownerId", "isAnonymous", "startAt"]
      },
      "Room": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "room_12345",
            "description": "ルームID"
          },
          "ownerId": {
            "type": "string",
            "example": "user_12345",
            "description": "オーナーのユーザーID"
          },
          "itemGetTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T00:00:00Z",
            "description": "アイテム取得日時"
          },
          "password": {
            "type": "string",
            "nullable": true,
            "example": "securepassword",
            "description": "ルームのパスワード"
          },
          "isAnonymous": {
            "type": "boolean",
            "example": false,
            "description": "匿名モードかどうか"
          },
          "startAt": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T00:00:00Z",
            "description": "開始日時"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T00:00:00Z",
            "description": "ルーム作成日時"
          },
          "generateCount": {
            "type": "number",
            "example": 0,
            "description": "生成回数"
          },
          "editId": {
            "type": "string",
            "example": "edit_12345",
            "description": "ルームの編集ID"
          }
        },
        "required": [
          "id",
          "ownerId",
          "itemGetTime",
          "password",
          "isAnonymous",
          "startAt",
          "createdAt",
          "generateCount",
          "editId"
        ]
      },
      "UpdateRoom": {
        "type": "object",
        "properties": {
          "itemGetTime": {
            "type": "string",
            "format": "date-time",
            "example": "2024-01-01T00:00:00Z",
            "description": "アイテム取得日時"
          },
          "password": {
            "type": "string",
            "example": "securepassword",
            "description": "ルームのパスワード"
          },
          "isAnonymous": {
            "type": "boolean",
            "example": false,
